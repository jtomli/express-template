<ul>
  <li class="list-group-item">
    <div class="container">
      <div class="row">
        <div class="col l6 s12">
          <a href="/venue/{{this.name}}"><h3>{{this.name}}</h3></a>
          <img src='{{this.link}}'>
        </div>
        <div class="col l6 s12">
          <h5>Info</h5>
          <ul>
            <li><b>Address: </b><a href="{{venue.url}}">{{venue.address}} </a></li>
            <li><b>Phone Number:</b> {{venue.phone}} </li>
            <li><b>Rating:</b> {{venue.rating}} </li>
            <li><b>Website:</b> <a href="{{venue.website}}">{{venue.website}} </a> </li>
            <li><b>Hours:</b>
                {{#each venue.hours}}
                  <p>{{this}}</p>
                {{/each}}
            </li>
          </ul>
        </div>
      </div>
      <a class="btn">Add to MyVenues</a>
    </div>
  </li>
</ul>
